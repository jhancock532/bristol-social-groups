{
  "name": "social-bristol",
  "version": "0.1.0",
  "private": true,
  "resolutions": {
    "eslint-plugin-react-hooks": "4.6.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "npm run sitemap && next build",
    "sitemap": "node scripts/generate-sitemap.js",
    "start": "next start",
    "test": "jest",
    "lint": "eslint . --resolve-plugins-relative-to node_modules && prettier --check '**/*.{md,css,scss,js,ts,tsx,json,yaml,yml}' && stylelint **/*.scss",
    "format": "prettier --write '**/?(.)*.{md,css,scss,js,ts,tsx,json,yaml,yml}'",
    "pre-commit-tasks": "npm run lint && npm test",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test-storybook": "test-storybook",
    "docker-playwright": "docker run --rm -p 9323:9323 -v $(pwd):/work/ -w /work/ -it mcr.microsoft.com/playwright:v1.52.0-jammy /bin/bash -c 'npm i; /bin/bash'",
    "playwright": "npx playwright test",
    "make-migration": "tsx scripts/make-migration.ts",
    "apply-migration": "tsx scripts/apply-migration.ts",
    "prepare": "husky"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "0.30.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@types/node": "20.5.6",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "dotenv": "16.4.5",
    "ejs": "3.1.10",
    "eslint-config-torchbox": "^1.1.0",
    "express": "4.21.2",
    "fuse.js": "7.0.0",
    "gray-matter": "^4.0.3",
    "leaflet": "1.9.4",
    "next": "14.2.3",
    "prettier": "3.2.4",
    "prettier-eslint": "16.3.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-leaflet": "4.2.1",
    "remark": "^15.0.1",
    "remark-html": "^16.0.1",
    "sass": "1.70.0",
    "tsx": "4.19.1",
    "typescript": "5.3.3",
    "yargs": "17.7.2"
  },
  "devDependencies": {
    "@babel/core": "^7.27.4",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-react": "^7.27.1",
    "@playwright/test": "1.46.0",
    "@storybook/addon-a11y": "8.2.7",
    "@storybook/addon-essentials": "8.2.7",
    "@storybook/addon-interactions": "8.2.7",
    "@storybook/addon-links": "8.2.7",
    "@storybook/addon-onboarding": "8.2.7",
    "@storybook/addon-styling-webpack": "1.0.0",
    "@storybook/blocks": "8.2.7",
    "@storybook/nextjs": "8.2.7",
    "@storybook/react": "8.2.7",
    "@storybook/test": "8.2.7",
    "@storybook/test-runner": "0.19.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/leaflet": "1.9.8",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "axe-playwright": "2.0.3",
    "babel-jest": "^30.0.0",
    "babel-preset-next": "^1.4.0",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "^13.5.6",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest-dom": "^5.5.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-storybook": "0.8.0",
    "eslint-plugin-testing-library": "^5.10.1",
    "husky": "9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.0.0",
    "storybook": "8.2.7",
    "stylelint": "16.8.1",
    "stylelint-config-css-modules": "4.4.0",
    "stylelint-config-standard": "36.0.1",
    "stylelint-config-standard-scss": "13.1.0",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2"
  }
