{
  "name": "social-bristol",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npm run sitemap && next build",
    "sitemap": "node scripts/generate-sitemap.js",
    "start": "next start",
    "lint": "next lint && prettier --check '**/?(.)*.{md,css,scss,js,ts,tsx,json,yaml,yml}' && stylelint '**/*.scss'",
    "format": "prettier --write '**/?(.)*.{md,css,scss,js,ts,tsx,json,yaml,yml}'",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test-storybook": "test-storybook",
    "docker-playwright": "docker run --rm -p 9323:9323 -v $(pwd):/work/ -w /work/ -it mcr.microsoft.com/playwright:v1.52.0-jammy /bin/bash -c 'npm i; /bin/bash'",
    "playwright": "npx playwright test",
    "make-migration": "tsx scripts/make-migration.ts",
    "apply-migration": "tsx scripts/apply-migration.ts",
    "prepare": "husky"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "0.54.0",
    "@types/node": "20.5.6",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "dotenv": "16.4.5",
    "ejs": "3.1.10",
    "eslint": "8.56.0",
    "eslint-config-next": "14.2.3",
    "express": "4.21.2",
    "fuse.js": "7.0.0",
    "gray-matter": "4.0.3",
    "leaflet": "1.9.4",
    "next": "14.2.22",
    "prettier": "3.2.4",
    "prettier-eslint": "16.3.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-leaflet": "4.2.1",
    "remark": "15.0.1",
    "remark-html": "16.0.1",
    "sass": "1.70.0",
    "tsx": "4.19.1",
    "typescript": "5.8.3",
    "yargs": "17.7.2"
  },
  "devDependencies": {
    "@playwright/test": "1.46.0",
    "@storybook/addon-a11y": "9.0.10",
    "@storybook/addon-docs": "9.0.10",
    "@storybook/addon-links": "9.0.10",
    "@storybook/addon-onboarding": "9.0.10",
    "@storybook/addon-styling-webpack": "2.0.0",
    "@storybook/nextjs": "9.0.10",
    "@storybook/test-runner": "0.23.0",
    "@types/leaflet": "1.9.8",
    "axe-playwright": "2.0.3",
    "eslint-config-torchbox": "1.1.0",
    "husky": "9.1.7",
    "storybook": "9.0.10",
    "stylelint": "16.8.1",
    "stylelint-config-css-modules": "4.4.0",
    "stylelint-config-standard": "36.0.1",
    "stylelint-config-standard-scss": "13.1.0"
  },
  "overrides": {
    "storybook": "$storybook"
  }
}
